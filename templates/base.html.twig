    :root {
        --color-black: #000000;
        --color-grey: #888888;
        --color-white: #FFFFFF;
    }

    * {
        font-weight: lighter;
    }

    a {
        color: var(--color-black);
    }

        a:hover {
            color: var(--color-grey);
        }

/* ** BASE - FOOTER ** /

    #copyright {
        font-size: 0.75em;
    }

